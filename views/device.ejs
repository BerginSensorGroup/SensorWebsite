                        <h4>Data</h4>
                        <a href = "/data/device/add"> Add Device </a>
                        <form method="post" action="/data">
                            <div>
                                <label for="device_id">Device:</label>
                                <br>
                                <input type="text" name="device_id" id="device_id" required>
                            </div>
                            <div>
                                <label for="begin_date">Begin Date:</label>
                                <br>
                                <input type="text" name="begin_date" id="begin_date" placeholder="YYYY-MM-DD">
                            </div>
                            <div>
                                <label for="end_date">End Date:</label>
                                <br>
                                <input type="text" name="end_date" id="end_date" placeholder="YYYY-MM-DD">
                            </div>

                            <p>Type of Data
                                <select type="text" id="data_type" name="data_type" class="form-control">
                                    <option value="Graphical">Graphical</option>
                                    <option value="Download-CSV">Download-CSV</option>
                                    <option value="Upload-CSV">Upload-CSV</option>
                                </select>
                            </p>
                            <div class="button">
                                <button type="submit" class="btn btn-outline-primary">Submit</button>
                            </div>
                        </form>
